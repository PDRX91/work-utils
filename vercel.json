{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "config": {
        "distDir": "dist"
      },
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "dest": "/server.js",
      "src": "/api/(.*)"
    },
    {
      "dest": "/dist/$1",
      "src": "/(.*)"
    }
  ],
  "version": 2
}
